{
  "name": "northstar",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.17.0",
  "scripts": {
    "dev": "SERWIST_SUPPRESS_TURBOPACK_WARNING=1 next dev",
    "build": "SERWIST_SUPPRESS_TURBOPACK_WARNING=1 next build",
    "build:clean": "rm -rf .next && SERWIST_SUPPRESS_TURBOPACK_WARNING=1 next build",
    "start": "next start",
    "lint": "eslint",
    "test": "NODE_ENV=test vitest",
    "test:ui": "NODE_ENV=test vitest --ui",
    "test:run": "NODE_ENV=test vitest run",
    "test:coverage": "vitest run --coverage",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "db:init": "psql postgresql://postgres:postgres@127.0.0.1:54322/postgres -c 'CREATE DATABASE northstar;' || true",
    "db:migrate:local": "DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/northstar npx drizzle-kit migrate",
    "db:setup": "pnpm db:init && pnpm db:migrate:local",
    "db:seed": "DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/northstar npx tsx drizzle/seed.ts",
    "db:reset": "DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/northstar npx tsx drizzle/db-reset.ts"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@hugeicons/core-free-icons": "^3.1.1",
    "@hugeicons/react": "^1.1.4",
    "@marsidev/react-turnstile": "^1.4.1",
    "@preact/signals": "^2.5.1",
    "@schedule-x/calendar": "^3.6.0",
    "@schedule-x/event-modal": "^3.6.0",
    "@schedule-x/events-service": "^3.6.0",
    "@schedule-x/react": "^3.5.0",
    "@schedule-x/theme-default": "^3.6.0",
    "@schedule-x/theme-shadcn": "^3.6.0",
    "@serwist/next": "^9.4.2",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "next": "16.1.1",
    "next-intl": "^4.7.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "preact": "^10.28.2",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rrule": "^2.8.1",
    "serwist": "^9.4.2",
    "shadcn": "^3.6.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "temporal-polyfill": "^0.3.0",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "dotenv": "^16.6.1",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "happy-dom": "^20.0.11",
    "tailwindcss": "^4",
    "tsx": "^4",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
